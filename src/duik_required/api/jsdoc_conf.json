{
    "plugins": [],
    "recurseDepth": 1,
    "source": {
        "includePattern": ".+\\.jsxinc$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "copyright"             : "Copyright (c) 2017-2019 Nicolas Dufresne, Rainbox Productions",
        "includeDate"           : true,
        "theme"                 : "slate",
        "collapseSymbols"       : "false",
        "syntaxTheme"           : "dark",
        "search"                : "true"
    },
    "opts": {
        "template": "tui-jsdoc-template/",
        "recurse": false,
        "destination": "../../../docs"
    }
}
