<!--


This control is valid for use in the PresetEffects.xml file,
however, keep in mind that some control properties are only
available when applying through the Pseudo Effect Maker and
will not have any effect when imported through the
PresetEffects.xml file. These are the exceptions for your control:

>	point_x point_y and (if using a Point3D) point_z properties will have no effect

>	Text Controls will work fine, but they will appear as empty groups in the XML

>	Dimmed text is not available


-->

<Effect matchname="Pseudo/DUFRAME grid" name="Grid">
	<Popup name="$$$/AE/Preset/Grid=Grid" default="1" popup_string="Safe Frames|Digital Frames|Center|Cross|Slanted Cross|Thirds|Slanted Thirds - Right|Slanted Thirds - Left|Slanted Thirds - Top|Slanted Thirdts - Bottom|Golden Rectangle|Golden Fibonacci|Real Fibonacci|Isometric" CANNOT_TIME_VARY="true" />
	<Point name="$$$/AE/Preset/Offset=Offset" default_x="0" default_y="0" point_x="0" point_y="0" />
	<Slider name="$$$/AE/Preset/Scale=Scale" default="100" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="1" DISPLAY_PERCENT="true" />
	<Angle name="$$$/AE/Preset/Rotation=Rotation" default="0" />
	<Slider name="$$$/AE/Preset/Opacity=Opacity" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100" precision="0" DISPLAY_PERCENT="true" />
	<Slider name="$$$/AE/Preset/StrokeSize=Stroke Size" default="2" valid_min="0" valid_max="10000" slider_min="0" slider_max="100" precision="1" />
	<Color name="$$$/AE/Preset/Color=Color" default_red="0" default_green="0" default_blue="0" />
	<Group name="$$$/AE/Preset/Duplicatethiseffect=Duplicate this effect" dim="true"  />
	<Group name="$$$/AE/Preset/toaddmoregrids.=to add more grids." dim="true"  />
</Effect>
