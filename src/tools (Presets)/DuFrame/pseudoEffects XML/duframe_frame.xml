<!--


This control is valid for use in the PresetEffects.xml file,
however, keep in mind that some control properties are only
available when applying through the Pseudo Effect Maker and
will not have any effect when imported through the
PresetEffects.xml file. These are the exceptions for your control:

>	point_x point_y and (if using a Point3D) point_z properties will have no effect


-->

<Effect matchname="Pseudo/DUFRAME frame" name="Frame">
	<Popup name="$$$/AE/Preset/FormatPreset=Format Preset" default="11" popup_string="Custom|Composition|Square (1)|4/3 (1.33)|Super 8 (1.35)|IMAX (1.43)|Super 16 / VistaVision (1.66)|16/9 (1.78)|DCP Flat (1.85)|Panavision 70 (2.20)|Panavision (2.35)|DCP Scope (2.39)|CinemaScope w/ magnetic sound (2.55)|Ultra Panavision 70 (2.76)" CANNOT_TIME_VARY="true" />
	<Slider name="$$$/AE/Preset/Format=Format" default="2.39" valid_min="0" valid_max="100" slider_min="0" slider_max="10" precision="2" />
	<Group name="$$$/AE/Preset/Appearance=Appearance" >
		<Popup name="$$$/AE/Preset/Mode=Mode" default="1" popup_string="Matte|Stroke" CANNOT_TIME_VARY="true" />
		<Color name="$$$/AE/Preset/Color=Color" default_red="0" default_green="0" default_blue="0" />
		<Slider name="$$$/AE/Preset/Opacity=Opacity" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100" precision="0" DISPLAY_PERCENT="true" />
		<Slider name="$$$/AE/Preset/StrokeSize=Stroke Size" default="0" valid_min="0" valid_max="10000" slider_min="0" slider_max="100" precision="1" />
	</Group>
	<Point name="$$$/AE/Preset/Computedsize=Computed size" default_x="0" default_y="0" point_x="0" point_y="0" />
</Effect>
