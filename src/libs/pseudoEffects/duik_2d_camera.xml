<!--


This control is valid for use in the PresetEffects.xml file,
however, keep in mind that some control properties are only
available when applying through the Pseudo Effect Maker and
will not have any effect when imported through the
PresetEffects.xml file. These are the exceptions for your control:

>	Text Controls will work fine, but they will appear as empty groups in the XML

>	Dimmed text is not available


-->

<Effect matchname="Pseudo/DUIK 2d camera" name="2D Camera">
	<Slider name="$$$/AE/Preset/HorizontalPan=Horizontal Pan" default="0" valid_min="-32000" valid_max="32000" slider_min="-2000" slider_max="2000" precision="1" />
	<Slider name="$$$/AE/Preset/VerticalPan=Vertical Pan" default="0" valid_min="-32000" valid_max="32000" slider_min="-2000" slider_max="2000" precision="1" />
	<Slider name="$$$/AE/Preset/Zoom=Zoom" default="0" valid_min="-1000" valid_max="1000" slider_min="-100" slider_max="100" precision="1" DISPLAY_PERCENT="true" />
	<Slider name="$$$/AE/Preset/TruckIn/Out=Truck In/Out" default="0" valid_min="-1000" valid_max="1000" slider_min="-100" slider_max="100" precision="1" DISPLAY_PERCENT="true" />
	<Angle name="$$$/AE/Preset/Tilt=Tilt" default="0" />
	<Group name="$$$/AE/Preset/Movethelayertotruck=Move the layer to truck" dim="true"  />
	<Group name="$$$/AE/Preset/(animateitsposition)=(animate its position)" dim="true"  />
	<Group name="$$$/AE/Preset/AdvancedBehaviour=Advanced Behaviour" >
		<Popup name="$$$/AE/Preset/Type=Type" default="1" popup_string="Fixed|Hand|Shoulder|Tripod|Steady" CANNOT_TIME_VARY="true" />
		<Group name="$$$/AE/Preset/GeneralMotion=General Motion" >
			<Slider name="$$$/AE/Preset/Amplitude=Amplitude" default="5" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1" DISPLAY_PERCENT="true" />
			<Slider name="$$$/AE/Preset/LoopDuration=Loop Duration" default="0.25" valid_min="0" valid_max="10000" slider_min="0" slider_max="30" precision="2" />
		</Group>
		<Group name="$$$/AE/Preset/Shake=Shake" >
			<Slider name="$$$/AE/Preset/Amplitude=Amplitude" default="0" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1" DISPLAY_PERCENT="true" />
			<Slider name="$$$/AE/Preset/Frequency=Frequency" default="15" valid_min="-100" valid_max="100" slider_min="-10" slider_max="10" precision="2" />
		</Group>
		<Group name="$$$/AE/Preset/Advanced=Advanced" >
			<Slider name="$$$/AE/Preset/RandomSeed=Random Seed" default="0" valid_min="0" valid_max="10000" slider_min="0" slider_max="10000" precision="0" />
		</Group>
	</Group>
</Effect>
