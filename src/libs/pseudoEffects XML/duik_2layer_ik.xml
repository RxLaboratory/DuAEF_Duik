<!--


This control is valid for use in the PresetEffects.xml file,
however, keep in mind that some control properties are only
available when applying through the Pseudo Effect Maker and
will not have any effect when imported through the
PresetEffects.xml file. These are the exceptions for your control:

>	Checkbox labels will not appear

>	point_x point_y and (if using a Point3D) point_z properties will have no effect


-->

<Effect matchname="Pseudo/DUIK 2layer ik" name="IK">
	<Checkbox name="$$$/AE/Preset/IK=IK" checked="true" label="Enabled" />
	<Checkbox name="$$$/AE/Preset/Reverse=Reverse" checked="false" />
	<Group name="$$$/AE/Preset/FK=FK" >
		<Group name="$$$/AE/Preset/Followthrough-Overlap=Follow through - Overlap" >
			<Angle name="$$$/AE/Preset/FK=FK" default="0" />
			<Slider name="$$$/AE/Preset/Flexibility=Flexibility" default="100" valid_min="0" valid_max="1000" slider_min="0" slider_max="200" precision="0" DISPLAY_PERCENT="true" />
			<Slider name="$$$/AE/Preset/Resistance=Resistance" default="10" valid_min="-1000" valid_max="1000" slider_min="-100" slider_max="200" precision="1" />
		</Group>
		<Group name="$$$/AE/Preset/Individualcontrols=Individual controls" >
			<Angle name="$$$/AE/Preset/Upper=Upper" default="0" />
			<Angle name="$$$/AE/Preset/Lower=Lower" default="0" />
			<Angle name="$$$/AE/Preset/Goal=Goal" default="0" />
		</Group>
		<Checkbox name="$$$/AE/Preset/Parentrotation=Parent rotation" checked="false" label="Inherit" />
	</Group>
	<Group name="$$$/AE/Preset/Stretch=Stretch" >
		<Checkbox name="$$$/AE/Preset/Auto-Stretch=Auto-Stretch" checked="true" />
		<Slider name="$$$/AE/Preset/Stretch=Stretch" default="0" valid_min="-10000" valid_max="10000" slider_min="-100" slider_max="100" precision="1" />
		<Slider name="$$$/AE/Preset/Auto-Shrink=Auto-Shrink" default="0" valid_min="0" valid_max="200" slider_min="0" slider_max="100" precision="1" DISPLAY_PERCENT="true" />
	</Group>
	<Group name="$$$/AE/Preset/Advanced=Advanced" >
		<Angle name="$$$/AE/Preset/Fullrotationlimit=Full rotation limit" default="0" />
	</Group>
	<Group name="$$$/AE/Preset/Data=Data" >
		<Group name="$$$/AE/Preset/Layers=Layers" >
			<Layer name="$$$/AE/Preset/Upper=Upper" default_self="false" />
			<Layer name="$$$/AE/Preset/Lower=Lower" default_self="false" />
			<Layer name="$$$/AE/Preset/Goal=Goal" default_self="false" />
		</Group>
		<Group name="$$$/AE/Preset/Stretchdata=Stretch data" >
			<Slider name="$$$/AE/Preset/Upperstretch=Upper stretch" default="0" valid_min="-32000" valid_max="32000" slider_min="-2000" slider_max="2000" precision="2" />
			<Slider name="$$$/AE/Preset/Upperscale=Upper scale" default="100" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="0" DISPLAY_PERCENT="true" />
			<Slider name="$$$/AE/Preset/Lowerstretch=Lower stretch" default="0" valid_min="-32000" valid_max="32000" slider_min="-2000" slider_max="2000" precision="2" />
			<Slider name="$$$/AE/Preset/Lowerscale=Lower scale" default="100" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="0" DISPLAY_PERCENT="true" />
			<Point name="$$$/AE/Preset/Goalposition=Goal position" default_x="0.5" default_y="0.5" point_x="0" point_y="0" />
			<Point name="$$$/AE/Preset/Goalworldposition=Goal world position" default_x="0.5" default_y="0.5" point_x="0" point_y="0" />
		</Group>
		<Group name="$$$/AE/Preset/Lengths=Lengths" >
			<Slider name="$$$/AE/Preset/Upperlength=Upper length" default="0" valid_min="0" valid_max="32000" slider_min="0" slider_max="2000" precision="2" />
			<Slider name="$$$/AE/Preset/Lowerlength=Lower length" default="0" valid_min="0" valid_max="32000" slider_min="0" slider_max="2000" precision="2" />
			<Slider name="$$$/AE/Preset/IKlength=IK length" default="0" valid_min="0" valid_max="32000" slider_min="0" slider_max="2000" precision="2" />
		</Group>
		<Checkbox name="$$$/AE/Preset/Straight=Straight" checked="false" />
	</Group>
	<Group name="$$$/AE/Preset/Display=Display" >
		<Checkbox name="$$$/AE/Preset/Drawguides=Draw guides" checked="true" />
	</Group>
</Effect>
