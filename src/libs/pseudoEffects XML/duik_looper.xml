<!--


This control is valid for use in the PresetEffects.xml file,
however, keep in mind that some control properties are only
available when applying through the Pseudo Effect Maker and
will not have any effect when imported through the
PresetEffects.xml file. These are the exceptions for your control:

>	Checkbox labels will not appear


-->

<Effect matchname="Pseudo/DUIK looper" name="Looper">
	<Group name="$$$/AE/Preset/In=In" >
		<Checkbox name="$$$/AE/Preset/In=In" checked="true" label="Activate" />
		<Popup name="$$$/AE/Preset/Type=Type" default="1" popup_string="Cycle|Offset|Ping Pong|Continue" />
	</Group>
	<Group name="$$$/AE/Preset/Out=Out" >
		<Checkbox name="$$$/AE/Preset/Out=Out" checked="true" label="Activate" />
		<Popup name="$$$/AE/Preset/Type=Type" default="1" popup_string="Cycle|Offset|Ping Pong|Continue" />
	</Group>
</Effect>
