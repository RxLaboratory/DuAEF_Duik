function buildAnimationPanelUI( tab )
{
    // A Spacer
    var spacer = tab.add('group');
    spacer.margins = 0;
    spacer.spacing = 0;
    spacer.size = [-1,3];

    var nlaButton = DuScriptUI.button(
        tab,
        DuScriptUI.String.NLA,
        w16_nla,
        DuScriptUI.String.NLA_TIP
    );
    nlaButton.onClick = Duik.Animation.setupNLA;
}