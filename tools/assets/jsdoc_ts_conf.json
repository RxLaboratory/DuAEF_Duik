{
    "plugins": [
        "node_modules/tsd-jsdoc/dist/plugin"
    ],
    "recurseDepth": 1,
    "source": {
		"include": [ #includepath# ],
        "includePattern": ".+\\.jsxinc$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
	"opts": {
        "template": "node_modules/tsd-jsdoc/dist",
		"recurse": false,
        "destination": "#destpath#"
    }
}