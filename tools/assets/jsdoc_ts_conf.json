{
    "plugins": [
        "node_modules/tsd-jsdoc/dist/plugin"
    ],
    "recurseDepth": 1,
    "source": {
		"include": [
            "../../build/Duik_API/libs/api1.jsxinc",
            "../../build/Duik_API/libs/api2.jsxinc",
            "../../build/Duik_API/libs/api3.jsxinc",
            "../../build/Duik_API/libs/DuAEF.jsxinc",
            "../../build/Duik_API/libs/DuGR_api.jsxinc",
            "../../build/Duik_API/libs/DuIO_api.jsxinc",
            "../../build/Duik_API/libs/DuSan_api.jsxinc"
        ],
        "includePattern": ".+\\.jsxinc$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
	"opts": {
        "template": "node_modules/tsd-jsdoc/dist",
		"recurse": false,
        "destination": "../../types/duik"
    }
}