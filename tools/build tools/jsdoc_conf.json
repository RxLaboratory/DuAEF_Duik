{
    "plugins": [],
    "recurseDepth": 1,
    "source": {
        "include": ["output/Duik_API/DuAEF_Duik_api.jsxinc"],
        "includePattern": ".+\\.jsxinc$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "copyright"             : "Copyright (c) 2017-2021 Nicolas Dufresne and contributors",
        "includeDate"           : true,
        "search"                : "true"
    },
    "meta": {
        "title": "Duik",
        "description": "Rigging and animation tool set for After Effects",
        "keyword": "ExtendScript, Adobe, After Effects"
    },
    "opts": {
        "template": "../../DuAEF/DuESF/tools/build/node_modules/jaguarjs-jsdoc",
        "recurse": false,
        "destination": "../../docs"
    }
}
